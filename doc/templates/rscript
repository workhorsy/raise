def setup():
	if os.path.isdir('example'):
		shutil.rmtree('example')
	os.mkdir('example')

	if os.path.isfile('one'):
		os.remove('one')

	if os.path.isfile('two'):
		os.remove('two')

	open('one', 'a').close()

def fs_change_dir():
	FS.change_dir('example')

def fs_move_file():
	FS.move_file('one', 'two')

